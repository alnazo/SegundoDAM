<Window x:Class="interfazenC_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:interfazenC_"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle Fill="Red">
            <Rectangle.Margin>
                0,0,500,300
            </Rectangle.Margin>
            <Rectangle.Width>300</Rectangle.Width>
            <Rectangle.Height>150</Rectangle.Height>
        </Rectangle>

        <Ellipse Fill="Yellow">
            <Ellipse.Margin>
                490,0,10,300
            </Ellipse.Margin>
            <Ellipse.Width>300</Ellipse.Width>
            <Ellipse.Height>150</Ellipse.Height>
        </Ellipse>

        <Button Width="150" Height="75" Margin="160,300,490,75" Background="Blue" Content="Aceptar" />

        <Button Background="Green">
            <Button.Content>Cancelar</Button.Content>
            <Button.Width>150</Button.Width>
            <Button.Height>75</Button.Height>
            <Button.Margin>
                490,300,160,75
            </Button.Margin>
        </Button>
    </Grid>
</Window>

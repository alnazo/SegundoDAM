<Window x:Class="BindingyClases.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BindingyClases"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="ComboBoxJugadoes" ItemsSource="{Binding Path=listaJugadores}" Height="30" Width="300" />
        <Button x:Name="BotonNuevo" Content="Nuevo" HorizontalAlignment="Center" VerticalAlignment="Top" Width="76" Margin="0,282,0,0" Click="BotonNuevo_Click" />
    </Grid>
</Window>
